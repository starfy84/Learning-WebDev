<div class="container">
  <div class="row">
    <div class="col-md-3 text-center">
      <%= image_tag @user.profile.avatar.url,class: 'avatar-img' %>
    </div>
    <div class="col-md-6">
      <h1><%= @user.profile.first_name %> <%= @user.profile.last_name%></h1>
      <h3><span class="education-title-icon"> <%= education_icon%></span><%= @user.profile.education%> Student</h3>
      <div class="well profile-block prof-desc">
        <h3>Description</h3>
        <%= @user.profile.description %>
      </div>
      <% if current_user.plan_id == 2 %>
      <div class="well profile-block prof-contact-info">
        <h3>Contact Information</h3>
        <%= @user.profile.phone_number %></br>
        <%= @user.profile.contact_email %></br>
      </div>
      <% end %>
      
    </div>
  </div>
</div>